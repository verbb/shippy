{
    "name": "verbb/shippy",
    "description": "A framework agnostic, multi-carrier shipping library for PHP.",
    "version": "1.0.5",
    "keywords": [
        "australia-post",
        "auspost",
        "ups",
        "usps",
        "fedex"
    ],
    "support": {
        "email": "support@verbb.io",
        "issues": "https://github.com/verbb/shippy/issues?state=open",
        "source": "https://github.com/verbb/shippy",
        "docs": "https://github.com/verbb/shippy",
        "rss": "https://github.com/verbb/shippy/commits/v2.atom"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Verbb",
            "homepage": "https://verbb.io"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "commerceguys/addressing": "^1.0||^2.0",
        "dvdoug/boxpacker": "^3.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/collections": "^8.0||^9.0||^10.0||^11.0",
        "php-units-of-measure/php-units-of-measure": "^2.0",
        "psr/log": "^1.0||^2.0||^3.0",
        "symfony/event-dispatcher": "^6.0||^7.0",
        "symfony/serializer": "^6.0||^7.0"
    },
    "require-dev": {
        "monolog/monolog": "^1.0||^2.0"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "verbb\\shippy\\": "src/"
        }
    }
}
